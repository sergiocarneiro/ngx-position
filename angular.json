{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-position": {
      "projectType": "library",
      "root": "projects/ngx-position",
      "sourceRoot": "projects/ngx-position/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngx-position/tsconfig.lib.json",
            "project": "projects/ngx-position/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-position/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-position/src/test.ts",
            "tsConfig": "projects/ngx-position/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-position/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-position/tsconfig.lib.json",
              "projects/ngx-position/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "cli": {
    "analytics": "ad318ffe-8e31-4aba-bf94-d7ac4e8383dd"
  },
  "defaultProject": "ngx-position"
}
